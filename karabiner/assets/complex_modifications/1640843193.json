{
  "title": "personal mapping 2",
  "rules": [
    {
      "description": "ctrl h for BS",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["left_control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right cmd quote tilde",
      "manipulators": [
        {
          "from": {
            "key_code": "quote",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Right command i for open bracket",
      "manipulators": [
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Right command o for close bracket",
      "manipulators": [
        {
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command ; for \\",
      "manipulators": [
        {
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command l for =",
      "manipulators": [
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command k for -",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command n for 6",
      "manipulators": [
        {
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "6"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command m for 7",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "7"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
